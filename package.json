{
    "name": "trubnik",
    "version": "0.0.3",
    "description": "mldigital project",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development DEBUG=0 webpack -d",
        "debug": "cross-env NODE_ENV=development DEBUG=1 webpack -d",
        "prod": "cross-env NODE_ENV=production DEBUG=0 webpack -p",
        "prod-debug": "cross-env NODE_ENV=production DEBUG=1 webpack -p",
        "watch": "npm run watch-dev",
        "watch-dev": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=0 webpack --watch -d\"",
        "watch-debug": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=1 webpack --watch -d\"",
        "js-lint": "cross-env NODE_ENV=production eslint --config .eslintrc.json --ignore-path .eslintignore --fix *.js source/**/*.js",
        "js-lint-config": "cross-env NODE_ENV=production eslint --print-config .eslintrc.json --ignore-path .eslintignore",
        "css-lint": "cross-env NODE_ENV=production stylelint source/**/*.scss --fix --syntax scss --formatter verbose --ignore-path .stylelintignore --config .stylelintrc.json",
        "css-lint-config": "cross-env NODE_ENV=production stylelint --syntax scss  --ignore-path .stylelintignore --print-config .stylelintrc.json",
        "html-validate": "cross-env NODE_ENV=production node ./script.html-validate.js --path-suffix",
        "html-validator": "cross-env NODE_ENV=production node ./script.html-validator.js --path-suffix",
        "app-lint": "cross-env NODE_ENV=production node ./script.app-lint.js",
        "lint": "npm run app-lint && npm run js-lint && npm run css-lint && npm run html-validate && npm run html-validator",
        "server": "npm run server-dev",
        "server-dev": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=0 webpack-dev-server --hot --inline -d\"",
        "server-prod": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=production DEBUG=0 webpack-dev-server --hot --inline -d\"",
        "server-debug": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=1 webpack-dev-server --hot --inline -d\"",
        "pre-build": "npm-run-all app-lint js-lint css-lint utf-fix svgo fonts-subsets",
        "post-build": "npm-run-all html-validate html-validator",
        "build": "npm-run-all pre-build prod post-build",
        "browserslist-dev": "cross-env NODE_ENV=development npx browserslist",
        "browserslist-prod": "cross-env NODE_ENV=production npx browserslist",
        "browserslist": "cross-env NODE_ENV=production npx browserslist",
        "fonts-subsets": "cross-env NODE_ENV=production node ./script.fonts-subsets.js",
        "utf-fix": "cross-env NODE_ENV=production node ./script.utf-fix.js",
        "svgo": "cross-env NODE_ENV=production node ./script.svgo.js",
        "start": "npm run build"
    },
    "author": "Denis Dolgov",
    "license": "MIT",
    "browserslist": {
        "production": [
            "Android >= 5",
            "IE >= 10",
            "iOS >= 9",
            "Safari >= 9",
            "last 2 Edge versions",
            "last 2 Chrome versions",
            "last 2 ChromeAndroid version",
            "last 2 Firefox versions",
            "last 2 Opera versions"
        ],
        "development": [
            "last 1 Safari versions",
            "last 1 Chrome versions",
            "last 1 Firefox versions",
            "last 1 Opera versions"
        ]
    },
    "engines": {
        "node": ">= 10.13.0"
    },
